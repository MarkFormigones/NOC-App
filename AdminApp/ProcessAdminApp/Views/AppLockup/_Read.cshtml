@model Hydron.Models.Definitions.AppLockupModel

<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12 profile-info">
                <h1 class="form-section">@Model.Name</h1>
                <ul class="list-unstyled">    
                    
                    <li>
                        <i class="fa fa-check-circle-o"></i><label class="control-label">Status:</label><a href="#">
                            @switch (Model.IsActive)
                            {
                                case true: <span class="label label-sm label-success">Active</span>
                                    break;
                                case false: <span class="label label-sm label-warning">Not Active</span>
                                    break;
                            }
                        </a>
                    </li>
                                                     
                    <li><i class="fa fa-calendar"></i><label class="control-label">Category Id:</label> @Model.CategoryId</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Date:</label> @Model.Dated</li>                  
                    <li><i class="fa fa-building"></i><label class="control-label">Description: </label></li>                                     
                    <li>@Model.Desc</li>
                    <li>

                    </li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Icon:</label> @Model.Icon</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Icon Class:</label> @Model.IconClass</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">General Class:</label> @Model.GeneralClass</li>
                </ul>
            </div>
        </div>    

                <!--end col-md-8-->
                <!--end col-md-4-->
            </div>
            <!--end row-->

          
        </div>







