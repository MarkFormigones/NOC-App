@model Hydron.Models.Definitions.NotificationLockupModel

<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12 profile-info">
                <h1 class="form-section">@Model.Name</h1>
                <ul class="list-unstyled">    
                    
                    <li>
                        <i class="fa fa-check-circle-o"></i><label class="control-label">Status:</label><a href="#">
                            @switch (Model.IsActive)
                            {
                                case true: <span class="label label-sm label-success">Active</span>
                                    break;
                                case false: <span class="label label-sm label-warning">Not Active</span>
                                    break;
                            }
                        </a>
                    </li>
                                                     
                    <li><i class="fa fa-calendar"></i><label class="control-label">Icon:</label> @Model.Icon</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">IconClass:</label> @Model.IconClass</li>                                    
                    <li><i class="fa fa-calendar"></i><label class="control-label">Tag:</label> @Model.Tag</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Type Id:</label> @Model.TypeId</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Dated:</label> @Model.Dated</li>
                    <li>
                        <i class="fa fa-check-circle-o"></i><label class="control-label">Is Escalatable:</label><a href="#">
                            @switch (Model.IsEscalatable)
                            {
                                case true: <span class="label label-sm label-success">Active</span>
                                    break;
                                case false: <span class="label label-sm label-warning">Not Active</span>
                                    break;
                            }
                        </a>
                    </li>
                    <li>
                        <i class="fa fa-check-circle-o"></i><label class="control-label">IsMentionDueDate:</label><a href="#">
                            @switch (Model.IsMentionDueDate)
                            {
                                case true: <span class="label label-sm label-success">Active</span>
                                    break;
                                case false: <span class="label label-sm label-warning">Not Active</span>
                                    break;
                            }
                        </a>
                    </li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect Controller:</label> @Model.RedirectController</li>
                    
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect Action:</label> @Model.RedirectAction</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect ExtraParam:</label> @Model.RedirectExtraParam</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect ExParam1:</label> @Model.RedirectExParam1</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect ExParam2:</label> @Model.RedirectExParam2</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect ExParam3:</label> @Model.RedirectExParam3</li>
                    <li><i class="fa fa-calendar"></i><label class="control-label">Redirect ExParamDefault:</label> @Model.RedirectExParamDefault</li>


                </ul>
            </div>
        </div>    

                <!--end col-md-8-->
                <!--end col-md-4-->
            </div>
            <!--end row-->

          
        </div>







