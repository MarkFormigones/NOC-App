                    <th><i class="fa fa-info"></i>    <label class="control-label"> Rate Plan Description: </label></th>
                    <td> @Model.RatePlanDesc</td>
                </tr>
                <tr>
                    <th><i class="fa fa-info"></i>  <label class="control-label"> Variable Amt MRC: </label></th>
                    <td> @Model.VariableAmount_MRC</td>
                </tr>
                <tr>
                    <th><i class="fa fa-info"></i>  <label class="control-label"> Variable Amt OTC: </label></th>
