                    <td> @Model.ContractName</td>
                </tr>
                <tr>
                    <th><i class="fa fa-info"></i> <label class="control-label"> Remarks: </label></th>
                    <td> @Model.ChargeDetails</td>
                </tr>
                <tr>
                    <th><i class="fa fa-info"></i>  <label class="control-label"> Approval Request: </label></th>
                    <td> @Model.ApprovalRequestStatusName</td>
                </tr>

                <tr>
                    <th><i class="fa fa-info"></i>  <label class="control-label"> CBCM Status: </label></th>
                    <td> @Model.CBCMStatusName</td>
                </tr>
               
