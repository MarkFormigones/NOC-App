@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

@section NotifyDocumentReady {


    showtoasteralert('@TempData["flag"]','@TempData["title"]','@TempData["statusMsg"]',  this);
    
     showtoasteralert('@TempData["innerFlag"]','@TempData["innerTitle"]','@TempData["innerMsg"]',  this);

}
<hgroup class="title">
   
    <h1 class="error">Opps, something happened while processing your request.</h1>

</hgroup>



<div class="row">
    <div class="col-md-2">
        <img class="ImagePreviewBox" src="~/assets/img/cool-error.png" />
        </div>
    <div class="col-md-10">
        <h4 class="error">@Model.ActionName</h4>
        <h4 class="error">@Model.ControllerName</h4>
        <p class="error">@TempData["innerTitle"]</p>
        <p class="error">@TempData["innerMsg"]</p>
        <p class="error">@DateTime.Now</p>
        <p class="error">@Model.Exception.Message</p>
    </div>
    </div>
